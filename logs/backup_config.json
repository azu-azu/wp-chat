{
  "enabled": true,
  "schedule": {
    "full_backup_days": 7,
    "incremental_hours": 24,
    "retention_days": 30,
    "max_backups": 10
  },
  "paths": {
    "index": "data/index/",
    "cache": "logs/cache/",
    "config": "config.yml",
    "logs": "logs/"
  },
  "compression": true,
  "verification": true,
  "auto_cleanup": true
}